    "express": "^4.16.4",
    "knex": "^0.16.3",
    "react-bulma-components": "^2.3.0",
    "react-router-dom": "^4.3.1",
    "reactstrap": "^7.1.0",
    "sqlite": "^3.0.1",
    "sqlite3": "^4.0.6",
    "style-loader": "^0.23.1",
    "superagent": "^4.1.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
